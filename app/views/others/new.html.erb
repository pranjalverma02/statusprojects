<h1>New Entry in 'Others'</h1>
<p>Please Enter Name from "Audit Paras Settled(Govt.& IA), Highlights, Leased Lines, Network Performance"</p>

<% form_for(@other) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :status %><br />
    <%= f.text_area :status %>
  </p>
  <p>
    <%= f.label :report_date %><br />
    <%= f.text_field :report_date %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', others_path %>
