<h1>Editing Entry in Project Status</h1>
<p>Please Enter name from "BWA project, CCTV of Avani Bhavan, Upgradation of Exchange, NOC"</p>

<% form_for(@statusproject) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :cost %><br />
    <%= f.text_field :cost %>
  </p>
  <p>
    <%= f.label :award_date %><br />
    <%= f.text_field :award_date %>
  </p>
  <p>
    <%= f.label :schd_date_of_compl %><br />
    <%= f.text_field :schd_date_of_compl %>
  </p>
  <p>
    <%= f.label :progress %><br />
    <%= f.text_area :progress %>
  </p>
  <p>
    <%= f.label :report_date %><br />
    <%= f.text_field :report_date %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @statusproject %> |
<%= link_to 'Back', statusprojects_path %>
