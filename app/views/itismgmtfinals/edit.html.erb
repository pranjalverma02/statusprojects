<h1>Editing Entry in IT Infrastructure Service Management-II</h1>
<p>Please Enter Name from "LAN Infrastructure(xclud PC), Tech Automation/New Innovation, Summer Trainee, Audio/Video support to Conf. Hall"</p>

<% form_for(@itismgmtfinal) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :existing_nos %><br />
    <%= f.text_field :existing_nos %>
  </p>
  <p>
    <%= f.label :change_in_nos %><br />
    <%= f.text_field :change_in_nos %>
  </p>
  <p>
    <%= f.label :availability_percentage %><br />
    <%= f.text_field :availability_percentage %>
  </p>
  <p>
    <%= f.label :downtime_hrs %><br />
    <%= f.text_field :downtime_hrs %>
  </p>
  <p>
    <%= f.label :report_date %><br />
    <%= f.text_field :report_date %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

